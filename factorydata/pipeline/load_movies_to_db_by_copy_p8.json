{
	"name": "load_movies_to_db_by_copy_p8",
	"properties": {
		"activities": [
			{
				"name": "load_movies_to_db_by_copy_p8",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "sampledata//moviesDB.csv"
					},
					{
						"name": "Destination",
						"value": "dbo.movies_copy"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "movie",
									"type": "String"
								},
								"sink": {
									"name": "id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "title",
									"type": "String"
								},
								"sink": {
									"name": "title",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "genres",
									"type": "String"
								},
								"sink": {
									"name": "genres",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "year",
									"type": "String"
								},
								"sink": {
									"name": "year",
									"type": "Int16"
								}
							},
							{
								"source": {
									"name": "Rating",
									"type": "String"
								},
								"sink": {
									"name": "rating",
									"type": "Int16"
								}
							},
							{
								"source": {
									"name": "Rotton Tomato",
									"type": "String"
								},
								"sink": {
									"name": "rotten_tomatoes",
									"type": "Int16"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "moviesDB",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "movies_copy_table",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}