{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myazudatafactory"
		},
		"myazusqldblinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'myazusqldblinkedservice'"
		},
		"myazustoragelinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'myazustoragelinkedservice'"
		},
		"myazudatalakestorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'myazudatalakestorage'"
		},
		"myazudatalakestorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myazustorage.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"myazusqldblinkedservice_connectionString": {
						"value": "[parameters('myazusqldblinkedservice_connectionString')]"
					},
					"myazustoragelinkedservice_connectionString": {
						"value": "[parameters('myazustoragelinkedservice_connectionString')]"
					},
					"myazudatalakestorage_accountKey": {
						"value": "[parameters('myazudatalakestorage_accountKey')]"
					},
					"myazudatalakestorage_properties_typeProperties_url": {
						"value": "[parameters('myazudatalakestorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}